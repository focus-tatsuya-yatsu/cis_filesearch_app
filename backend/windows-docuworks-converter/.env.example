# DocuWorks Converter Environment Variables
# Copy this file to .env and fill in the values

# AWS Configuration
AWS_REGION=ap-northeast-1
# AWS_ACCESS_KEY_ID=your-access-key  # Or use IAM role
# AWS_SECRET_ACCESS_KEY=your-secret-key

# S3 Configuration
S3_SOURCE_BUCKET=cis-filesearch-s3-landing
S3_TARGET_BUCKET=cis-filesearch-s3-thumbnail
S3_SOURCE_PREFIX=
S3_TARGET_PREFIX=converted-pdf/

# SQS Configuration (for service mode)
SQS_QUEUE_URL=https://sqs.ap-northeast-1.amazonaws.com/123456789012/docuworks-convert-queue

# Service Configuration
POLL_INTERVAL=10
MAX_FILE_SIZE_MB=100
