{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:clean": "bash scripts/dev-manager.sh restart",
    "dev:stop": "bash scripts/dev-manager.sh stop",
    "dev:status": "bash scripts/dev-manager.sh status",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:images": "node scripts/optimize-images.js",
    "build:production": "yarn build:images && yarn build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:image-search": "jest --testPathPattern=\"image\" --coverage",
    "test:unit": "jest --testPathPattern=\"\\.test\\.(ts|tsx)$\" --coverage",
    "test:integration": "jest --testPathPattern=\"route\\.test\\.ts$\" --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install --with-deps",
    "benchmark": "tsx scripts/performance-benchmark.ts",
    "benchmark:quick": "ITERATIONS=10 tsx scripts/performance-benchmark.ts",
    "benchmark:image": "ts-node scripts/benchmark-image-search.ts",
    "load-test": "artillery run scripts/load-test-config.yml",
    "load-test:report": "artillery run --output load-test-report.json scripts/load-test-config.yml && artillery report load-test-report.json",
    "migrate:opensearch": "tsx scripts/opensearch-secure-migration.ts",
    "migrate:opensearch:dry-run": "tsx scripts/opensearch-secure-migration.ts --dry-run",
    "test:image-integration": "tsx scripts/test-image-search-integration.ts",
    "test:image-integration:vpc": "VPC_MODE=true tsx scripts/test-image-search-integration.ts",
    "test:opensearch": "bash scripts/test-opensearch-image-search.sh",
    "test:opensearch:vpc": "VPC_MODE=true bash scripts/test-opensearch-image-search.sh",
    "test:vpc": "bash scripts/run-tests-vpc.sh",
    "test:all": "yarn test:unit && yarn test:integration && yarn test:image-integration && yarn test:e2e",
    "test:all:coverage": "yarn test:coverage && yarn test:e2e",
    "perf:test": "ts-node scripts/run-performance-test.ts",
    "perf:test:quick": "QUICK_MODE=true ts-node scripts/run-performance-test.ts",
    "perf:analyze": "node -e \"console.log('Performance analysis: See /docs/IMAGE_SEARCH_PERFORMANCE_IMPLEMENTATION_GUIDE.md')\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-amplify/auth": "^6.16.0",
    "@aws-amplify/core": "^6.13.3",
    "@aws-sdk/client-bedrock-runtime": "^3.948.0",
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/credential-provider-node": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.952.0",
    "@heroicons/react": "^2.2.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-virtual": "^3.13.13",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "aws-amplify": "^6.15.7",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "next": "^16.0.7",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@typescript-eslint/eslint-plugin": "7",
    "@typescript-eslint/parser": "7",
    "eslint": "8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "webpack-bundle-analyzer": "^5.1.0",
    "whatwg-fetch": "^3.6.20"
  }
}
